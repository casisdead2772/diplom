{
  "name": "bookstore-admin",
  "version": "0.5.7",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -rf ../public/{js,css,img,fonts} && vue-cli-service build --no-clean --mode production",
    "lint": "vue-cli-service lint ./src",
    "crud:make": "vue-cli-service crud:make",
    "crud:yaml": "vue-cli-service crud:yaml",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.4.4",
    "moment": "^2.29.1",
    "portal-vue": "^2.1.7",
    "vue": "^2.6.12",
    "vue-chartist": "^2.3.0",
    "vue-i18n": "^8.21.1",
    "vue-router": "^3.3.2",
    "vuedraggable": "^2.24.1",
    "vuetify": "^2.3.10",
    "vuetify-admin": "^0.5.7",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^7.0.0",
    "prettier": "^2.1.1",
    "sass": "^1.26.7",
    "sass-loader": "^10.0.2",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-cli-plugin-vuetify-admin": "^0.5.7",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.4.4"
  }
}
